{
    "name": "Custom Theme",
    "publisher": "GobySec",
    "description": "This extension can debug Goby GUI themes online and generate theme extensions with one click.",
    "version": "0.2.2",
    "icon": "src/assets/img/icon.png",
    "engines": "1.8.230",
    "initEvents": "",
    "main": "./src/extension.js",
    "contributes": {
        "configuration": {
            "name": {
                "type": "string",
                "default": "",
                "description": "Extension name"
            },
            "publisher": {
                "type": "string",
                "default": "",
                "description": "Extension publisher"
            },
            "description": {
                "type": "string",
                "default": "",
                "description": "Extension description"
            },
            "version": {
                "type": "string",
                "default": "",
                "description": "Extension version"
            },
            "icon": {
                "type": "string",
                "default": "",
                "description": "Extension icon",
                "fromDialog": true
            }
        },
        "views": {
            "toolbar": [
                {
                    "command": "ThemeDevelop",
                    "title": "ThemeDevelop",
                    "icon": "src/assets/img/icon.png"
                }
            ]
        }
    },
    "language": {
        "CN": "src/assets/translate/CN/translate.json"
    },
    "scripts": {},
    "devDependencies": {},
    "dependencies": {
        "color": "^3.1.3",
        "jszip": "^3.7.0",
        "mavon-editor": "^2.9.1",
        "rgb-hex": "^3.0.0"
    }
}